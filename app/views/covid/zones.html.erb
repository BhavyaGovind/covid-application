<%= render :partial => 'home' %>
<div class="center-block fix-width fix-height scroll-inner">
  <table class="table table-striped">
    <tr>
      <th>Suburb Name</th>
      <th>Zone</th>
    </tr>
    <% @suburb_by_cases.keys.each do |suburb| %>
      <tr>
        <td><%= suburb %></td>
        <% if @suburb_by_cases[suburb] < 10 %>
          <td><%= "Green" %></td>
        <%  elsif @suburb_by_cases[suburb] < 25 %>
          <td><%= "Amber" %></td>
        <% else %>
          <td><%= "Red" %></td>
        <% end %>
      </tr>
    <% end %>
  </table>

</div>
